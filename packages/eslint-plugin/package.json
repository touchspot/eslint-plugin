{
	"name": "@touchspot/eslint-plugin",
	"version": "1.4.1",
	"description": "TOUCHSPOT ESLint plugin",
	"keywords": [
		"eslint",
		"typescript"
	],
	"homepage": "https://github.com/touchspot/eslint-plugin/tree/main/packages/eslint-plugin",
	"bugs": {
		"url": "https://github.com/touchspot/eslint-plugin/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/touchspot/eslint-plugin.git",
		"directory": "packages/eslint-plugin"
	},
	"license": "MIT",
	"author": "mozisan <mozisan@users.noreply.github.com>",
	"type": "module",
	"exports": {
		".": {
			"require": "./dist/lib.cjs",
			"import": "./dist/lib.js"
		},
		"./package.json": "./package.json"
	},
	"main": "./dist/lib.cjs",
	"module": "./dist/lib.js",
	"files": [
		"dist",
		"package.json",
		"README.md"
	],
	"scripts": {
		"build": "pkgroll",
		"check:format": "prettier --check .",
		"check:lint": "eslint .",
		"check:type": "tsc --noEmit",
		"check:unused": "knip",
		"fix:format": "prettier --write .",
		"fix:lint": "eslint --fix ."
	},
	"dependencies": {
		"@next/eslint-plugin-next": "^14.2.4",
		"@typescript-eslint/eslint-plugin": "^7.14.1",
		"@typescript-eslint/parser": "^7.14.1",
		"@typescript-eslint/type-utils": "^7.14.1",
		"@typescript-eslint/utils": "^7.14.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-check-file": "^2.8.0",
		"eslint-plugin-functional": "^6.6.0",
		"eslint-plugin-import-x": "^0.5.2",
		"eslint-plugin-no-relative-import-paths": "^1.5.5",
		"eslint-plugin-perfectionist": "^2.11.0",
		"eslint-plugin-react": "^7.34.3",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-perf": "^3.3.2",
		"eslint-plugin-tailwindcss": "^3.17.4",
		"eslint-plugin-unicorn": "^53.0.0",
		"eslint-plugin-unused-imports": "^3.2.0"
	},
	"devDependencies": {
		"@tsconfig/node20": "^20.1.4",
		"@tsconfig/strictest": "^2.0.5",
		"eslint": "^8.57.0",
		"next": "^14.2.4",
		"react": "^18.3.1",
		"tailwindcss": "^3.4.4"
	},
	"peerDependencies": {
		"eslint": "^8.56.0",
		"next": "^14.1.0",
		"react": "^18.2.0",
		"tailwindcss": "^3.4.1"
	},
	"peerDependenciesMeta": {
		"next": {
			"optional": true
		},
		"react": {
			"optional": true
		},
		"react-dom": {
			"optional": true
		},
		"tailwindcss": {
			"optional": true
		}
	},
	"publishConfig": {
		"access": "public"
	}
}
